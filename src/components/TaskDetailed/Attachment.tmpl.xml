<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <div class="edit-task-content-attachments-body-element" id="{json.fileHtmlID}">
        <div class="edit-task-content-attachments-body-element-left">
            <img class="edit-task-content-attachments-body-element-left__icon" src="../../img/icons/file.svg"
                 id="{json.fileIconID}" data-outhref="{json.fileUrl}" alt="remove"/>

            <a class="edit-task-content-attachments-body-element-left__text" id="{json.fileNameID}"
               data-outhref="{json.fileUrl}">
                <fest:value>json.fileName</fest:value>
            </a>
        </div>

        <div class="edit-task-content-attachments-body-element-right">
            <div class="edit-task-content-attachments-body-element-right__delete"
                 id="{json.fileRemoveID}" alt="remove"> Удалить </div>

        </div>
    </div>
</fest:template>
