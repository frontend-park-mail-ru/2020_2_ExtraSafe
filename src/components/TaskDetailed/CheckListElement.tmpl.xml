<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <div class="edit-task-content-check-lists-body-element" id="{json.checkListElementHtmlID}">
        <img class="edit-task-content-check-lists-body-element__icon"
             id="{json.checkListElementCheckID}" alt="check">
            <fest:attributes>
                <fest:if test="json.isChecked">
                    <fest:attribute name="src">
                        ../../img/icons/check_grey.svg
                    </fest:attribute>
                </fest:if>
                <fest:attribute name="src">
                    ../../img/icons/box_unchecked.svg
                </fest:attribute>
            </fest:attributes>
        </img>

        <div class="edit-task-content-check-lists-body-element__text" id="{json.checkListElementNameID}"
             contenteditable="true">
            <fest:value>json.checkListElementName</fest:value>
        </div>
    </div>
</fest:template>
